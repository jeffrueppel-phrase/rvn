<?xml version="1.0" encoding="utf-8"?>
<CatapultSkin Version="4" SkinType="Component" Comment="HTML5 Toolbar skin" ComponentType="TopicToolbar" EnableResponsiveOutput="True">
    <Stylesheet>
        <Styles>
            <Style Name="ToolbarItem">
                <Classes>
                    <StyleClass Name="back-button" Custom="true">
                        <Properties>
                            <Property Name="BackgroundRepeat">no-repeat</Property>
                            <Property Name="BackgroundPosition">center center</Property>
                            <Property Name="ButtonIcon">url('home-icon-day.png')</Property>
                            <Property Name="Click">goBack()</Property>
                            <Property Name="Width">22px</Property>
                            <Property Name="Height">39px</Property>
                            <Property Name="BoxShadowColor">transparent</Property>
                            <Property Name="BoxShadowVShadow">0px</Property>
                            <Property Name="BackgroundColor">transparent</Property>
                            <Property Name="ButtonIconHeight">39px</Property>
                            <Property Name="ButtonIconWidth">22px</Property>
                        </Properties>
                    </StyleClass>
                    <StyleClass Name="CurrentTopicIndex">
                        <Properties>
                            <Property Name="BorderLeft">none 1px #e1e1e1</Property>
                            <Property Name="BorderRight">none 1px #e1e1e1</Property>
                            <Property Name="BorderTop">none 1px #e1e1e1</Property>
                            <Property Name="BorderBottom">none 1px #e1e1e1</Property>
                            <Property Name="TextShadowVisibility">hidden</Property>
                            <Property Name="BackgroundColor">transparent</Property>
                            <Property Name="Width">70px</Property>
                            <Property Name="Height">90px</Property>
                            <Property Name="ButtonIconHeight">90px</Property>
                            <Property Name="ButtonIconWidth">70px</Property>
                        </Properties>
                    </StyleClass>
                </Classes>
            </Style>
            <Medium Name="Tablet">
                <Style Name="ToolbarItem">
                    <Classes>
                        <StyleClass Name="back-button">
                            <Properties>
                                <Property Name="BackgroundRepeat">no-repeat</Property>
                                <Property Name="BackgroundPosition">center center</Property>
                            </Properties>
                        </StyleClass>
                    </Classes>
                </Style>
            </Medium>
            <Medium Name="Mobile">
                <Style Name="ToolbarItem">
                    <Classes>
                        <StyleClass Name="back-button">
                            <Properties>
                                <Property Name="BackgroundRepeat">no-repeat</Property>
                                <Property Name="BackgroundPosition">center center</Property>
                            </Properties>
                        </StyleClass>
                    </Classes>
                </Style>
            </Medium>
        </Styles>
        <Stylesheet>
        </Stylesheet>
        <Resources>
            <Resource Name="home-icon-day.png"></Resource>
        </Resources>
    </Stylesheet>
    <TopicToolbar EnableCustomLayout="true" Buttons="back-button">
        <Script>function goBack()
{
window.history.back();
}</Script>
    </TopicToolbar>
    <Strings>
        <Stylesheet lang="en-us">
            <Stylesheet>
                <Styles>
                    <Style Name="ToolbarItem">
                        <Classes>
                            <StyleClass Name="back-button">
                                <Properties>
                                    <Property Name="Tooltip">Back</Property>
                                </Properties>
                            </StyleClass>
                        </Classes>
                    </Style>
                </Styles>
            </Stylesheet>
        </Stylesheet>
    </Strings>
</CatapultSkin>